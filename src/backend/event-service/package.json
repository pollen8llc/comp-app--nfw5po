{
  "name": "@community-platform/event-service",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn src/app.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/app.js",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "docs:generate": "typedoc --out docs src",
    "security:audit": "pnpm audit",
    "proto:generate": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/proto src/proto/*.proto"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.10.0",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.9",
    "@openapi-types/openapi-types": "^12.1.3",
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-redis-cache": "^1.1.3",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "neo4j-driver": "^5.12.0",
    "neo4j-query-builder": "^2.0.0",
    "newrelic": "^10.3.0",
    "pino": "^8.15.0",
    "pino-pretty": "^10.0.0",
    "rate-limit-redis": "^3.0.0",
    "rate-limiter-flexible": "^2.4.1",
    "retry": "^0.13.0",
    "retry-axios": "^3.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@jest/globals": "^29.0.0",
    "@types/cors": "^2.8.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "eslint": "^8.39.0",
    "jest": "^29.0.0",
    "jest-mock-express": "^0.2.0",
    "nock": "^13.3.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/organization/community-platform.git"
  },
  "keywords": [
    "event-service",
    "microservice",
    "community-platform"
  ],
  "author": "Community Platform Team",
  "license": "UNLICENSED"
}